#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{char c;
char ch[20];
char sen[100];
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    scanf("%c", &c);
     scanf("%s", &ch);
     scanf("\n");
     scanf("%[^\n]s", &sen);
    printf("%c\n", c);
    printf("%s\n", ch);
    printf("%s", sen);
    
    return 0; 
}
