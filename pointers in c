#include <stdio.h>

void update(int *a,int *b)
 {
    // Complete this function  
       int s = *a;
       int r = *b;
       *a = s + r;
      *b = abs(s - r);
}

int main() {
    int a, b;
    int *pa = &a, *pb = &b;
    
    scanf("%d %d", &a, &b);
    update(pa, pb);
    printf("%d\n%d", a, b);

    return 0;
}
